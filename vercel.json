{
  "headers" : [
    {
      "source" : "/(.*)",
      "headers" : [
        {
          "key" : "Cache-Control",
          "value" : "public, max-age=604800, stale-while-revalidate=86400"
        }
      ]
    },
    {
      "source": "/manifest.appcache",
      "headers": [
        {
          "key": "Content-Type",
          "value": "text/cache-manifest"
        }
      ]
    }
  ]
}